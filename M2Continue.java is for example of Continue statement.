class M2Continue {
    public static void main(String[] args) {

        for (int i = 1; i <= 10; i++) {

            if (i == 5) {
                System.out.println("Skipping i = " + i);
                continue;   // skips this iteration
            }

            System.out.println("i = " + i);
        }

        System.out.println("Loop completed.");
    }
}
