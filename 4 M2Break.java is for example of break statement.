class M2Break {
    public static void main(String[] args) {

        for (int i = 1; i <= 10; i++) {

            if (i == 5) {
                System.out.println("Break encountered at i = " + i);
                break;  // stops the loop
            }

            System.out.println("i = " + i);
        }

        System.out.println("Loop terminated.");
    }
}
