// Base class
class Doctor {
    void consultationFee() {
        System.out.println("General Doctor Fee: ₹200");
    }
}

// Subclass 1
class Dentist extends Doctor {
    @Override
    void consultationFee() {
        System.out.println("Dentist Consultation Fee: ₹500");
    }
}

// Subclass 2
class Cardiologist extends Doctor {
    @Override
    void consultationFee() {
        System.out.println("Cardiologist Consultation Fee: ₹800");
    }
}

// Subclass 3
class Surgeon extends Doctor {
    @Override
    void consultationFee() {
        System.out.println("Surgeon Consultation Fee: ₹1000");
    }
}

// Main class
class InhrDoctorExample {
    public static void main(String[] args) {

        Doctor d;

        d = new Dentist();
        d.consultationFee();

        d = new Cardiologist();
        d.consultationFee();

        d = new Surgeon();
        d.consultationFee();
    }
}
