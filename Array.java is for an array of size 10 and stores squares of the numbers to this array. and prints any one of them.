public class Array {
    public static void main(String[] args) {

        // Array of size 10
        int[] squares = new int[10];

        // Store squares of numbers 1 to 10
        for (int i = 0; i < 10; i++) {
            squares[i] = (i + 1) * (i + 1);
        }

        // Print any one value from the array (example: 5th element)
        System.out.println("Square stored at index 4: " + squares[4]);
    }
}
